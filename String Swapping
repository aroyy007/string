//string swapping
#include<stdio.h>
#include<string.h>
int main()
{
    char str1[30],str2[30],temp[30];
    printf("Enter = ");
    gets(str1);

    printf("Enter = ");
    gets(str2);

    printf("Before Swapping\n");
    printf("Str1 = %s\n",str1);
    printf("Str2 = %s\n",str2);
    //copy
    strcpy(temp,str1);
    strcpy(str1,str2);
    strcpy(str2,temp);

    printf("After Swapping\n");
    printf("Str1 = %s\n",str1);
    printf("Str2 = %s\n",str2);
}
