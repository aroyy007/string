#include<stdio.h>
#include<string.h>
int main()
{
    char str1[30],str2[30];
    printf("Enter = ");
    gets(str1);
    printf("Str = %s\n",str1);
    strcpy(str2,str1);
    strrev(str1);
    printf("Reverse : %s\n",str1);
    int d = strcmp(str1,str2);
    if(d==0)
    {
        printf("palindrome");
    }
    else
    {
        printf("not palindrome");
    }
}
